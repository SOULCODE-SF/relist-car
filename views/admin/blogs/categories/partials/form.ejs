<div class="col-lg-12 col-md-12">
    <div class="card">
      <div class="card-body">
        <h5 class="card-title">Categories Form</h5>
        
        <div class="row g-3 mb-3">
          <label for="name" class="col-md-2 col-form-label">Name</label>
          <div class="col-md-6">
            <input
              name="name"
              type="text"
              class="form-control"
              id="name"
              placeholder="Enter category name"
              value="<%= data?.name %>"
            />
          </div>
        </div>

      </div>
    </div>
  </div>
  