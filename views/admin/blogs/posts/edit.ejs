<div class="pagetitle mb-2"></div>
    <div class="row align-items-center">
      <div class="col-md-8 mt-2">
        <h1 class="fs-4 mb-1">Edit Posts Form</h1>
        <nav>
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/admin">Home</a></li>
            <li class="breadcrumb-item"><a href="/admin/blog/posts">Posts List</a></li>
            <li class="breadcrumb-item active">Edit Posts Form</li>
          </ol>
        </nav>
      </div>
    </div>
  </div>
  
  <section class="section">
    <form
      class="row g-3"
      id="posts-form"
      action="/admin/blog/edit-posts/<%= data.id %>"
      method="post"
      enctype="multipart/form-data"
    >
      <%- include('partials/form') %>
      <div class="col-md-4 mb-2">
        <button type="submit" class="btn btn-primary">Save</button>
        <a href="/admin/blog/posts" class="btn btn-secondary">Cancel</a>
      </div>
    </form>
  </section>