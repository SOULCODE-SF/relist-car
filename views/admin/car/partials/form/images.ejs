<div class="col-lg-12 col-md-12">
  <div class="card">
    <div class="card-body">
      <h5 class="card-title">Images</h5>
      <div class="form-group d-grid gap-3">
        <div class="row g-3 mb-3" id="image-upload-container">
          <label for="image_upload" class="col-md-2 col-form-label">Images</label>
          <input
            name="images"
            type="file"
            id="input-images"
            accept="image/*"
            multiple
          />
        </div>
      </div>
    </div>
  </div>
</div>
