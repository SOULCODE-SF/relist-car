<div class="layout-brands">
  <ul class="nav nav-tabs">
    <li class="nav-item">
      <a href="/brands" class="nav-link">Brands</a>
    </li>
    <li class="nav-item">
      <span class="arrow">&rarr;</span>
    </li>
    <li class="nav-item">
      <a href="/brands/<%= data.general_information.brand_id %>/models" class="nav-link"
        ><%= data.general_information.brand_name %></a
      >
    </li>
    <li class="nav-item">
      <span class="arrow">&rarr;</span>
    </li>
    <li class="nav-item">
      <a href="/brands/models/<%= data.general_information.model_id %>/generations" class="nav-link"
        ><%= data.general_information.model_name %></a
      >
    </li>
    <li class="nav-item">
      <span class="arrow">&rarr;</span>
    </li>
    <li class="nav-item">
      <a href="/generation-list/<%= data.general_information.generation_id %>" class="nav-link"
        ><%= data.general_information.generation_name %></a
      >
    </li>
    <li class="nav-item">
      <span class="arrow">&rarr;</span>
    </li>
    <li class="nav-item">
      <a href="#" class="nav-link active"><%= data.general_information.engine%></a>
    </li>
  </ul>
</div>

<div class="container mt-5">
  <div class="detail">
    <table class="table table-bordered cardetailsout car2">
      <caption>
        <h2 class="car-name"><%= data.name %></h2>
      </caption>
      <tbody>
        <div class="container mt-4 gallery-container mb-4">
          <div class="row no-gutters">
            <% data.images.forEach(function(image) { %>
              <div class="col-md-2">
                <div class="gallery-item">
                  <img src="/assets/<%= image.image_path %>" alt="<%= image.meta_title %>">
                </div>
              </div>
            <% }); %>
          </div>
        </div>     
        <%- include('../partials/spec/general_information.ejs') %>
        <%- include('../partials/spec/performance_spec.ejs') %>
        <%- include('../partials/spec/engine_spec.ejs') %>
        <%- include('../partials/spec/electric_hybrid.ejs')%>
        <%- include('../partials/spec/space_weight_volume.ejs') %>
        <%- include('../partials/spec/dimension.ejs') %>
        <%- include('../partials/spec/drivetrain.ejs') %>
      </tbody>
    </table>
  </div>
</div>

